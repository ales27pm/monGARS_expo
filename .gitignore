!ios/Pods/**
!ios/Podfile.lock
# Keep DerivedData and build folders
!ios/DerivedData/**
!ios/build/**

# Android large outputs
android/.gradle/
android/build/
android/app/build/

# Expo log
expo.log

# Environment variables
.env
.env.local
.env.production
