{
  "cli": {
    "version": "16.26.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {},
      "ios": {
        "distribution": "store",
        "buildConfiguration": "Release",
        "simulator": false,
        "resourceClass": "m-medium"
      },
      "android": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "AUTO",
        "appleTeamId": "AUTO",
        "ascApiKeyPath": "./eas-credentials/asc-api-key.json"
      }
    }
  }
}
