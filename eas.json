{
  "cli": {
    "version": ">= 16.17.4",
    "requireCommit": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "channel": "preview"
    },
    "production": {
      "ios": {
        "distribution": "store",
        "resourceClass": "m-large",
        "buildConfiguration": "Release",
        "image": "latest"
      },
      "android": {
        "distribution": "store",
        "resourceClass": "large"
      },
      "channel": "production",
      "autoIncrement": true
    },
    "production-with-models": {
      "extends": "production",
      "ios": {
        "resourceClass": "large",
        "image": "latest"
      },
      "android": {
        "resourceClass": "large"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "${APPLE_ID}",
        "ascAppId": "${ASC_APP_ID}",
        "appleTeamId": "${APPLE_TEAM_ID}"
      }
    }
  }
}
