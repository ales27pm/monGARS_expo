{
  "expo": {
    "name": "MonGARS",
    "slug": "mongars",
    "scheme": "mongars",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mongars.app",
      "buildNumber": "5",
      "deploymentTarget": "16.0",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "MonGARS uses speech recognition to turn your spoken words into text inside the app.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mongars.app"
    },
    "icon": "./icon.png",
    "extra": {
      "eas": {
        "projectId": "f90e5e7c-1263-4959-8c90-12d08249de06"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-build-properties",
      "expo-font",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser"
    ]
  }
}
