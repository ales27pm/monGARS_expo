{
  "expo": {
    "name": "monGARS",
    "slug": "offllm-app-store-fixer",
    "scheme": "mongars",
    "version": "1.0.4",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.vibecode.offllmappstorefixer-iz6sup",
      "buildNumber": "12",
      "deploymentTarget": "16.0",
      "appStoreId": "6754946066",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "MonGARS uses speech recognition to turn your spoken words into text inside the app.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mongars.app"
    },
    "icon": "./icon.png",
    "extra": {
      "privacyPolicyUrl": "http://27pm.org",
      "eas": {
        "projectId": "f90e5e7c-1263-4959-8c90-12d08249de06"
      }
    },
    "plugins": [
      "expo-asset",
      "expo-build-properties",
      "expo-font",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser"
    ]
  }
}
